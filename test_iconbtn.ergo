bring stdr
bring cogito

fun build_ui(win = cogito.Window) (( -- )) {
    let vstack = cogito.vstack()
    win.add(vstack)

    let iconbtn = cogito.iconbtn(@"sf:star.fill")
    vstack.add(iconbtn)
}

entry () (( -- )) {
    let app = cogito.app()
    let window = cogito.window_title(@"Icon Test")
    window.build(build_ui)
    app.run(window)
}
