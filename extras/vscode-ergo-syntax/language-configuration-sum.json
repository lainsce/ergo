{
  "comments": {
    "lineComment": ";"
  },
  "brackets": [
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ]
  ],
  "autoClosingPairs": [
    {
      "open": "[",
      "close": "]"
    },
    {
      "open": "(",
      "close": ")"
    },
    {
      "open": "\"",
      "close": "\""
    }
  ],
  "surroundingPairs": [
    [
      "[",
      "]"
    ],
    [
      "(",
      ")"
    ],
    [
      "\"",
      "\""
    ]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^\\s*[^;\\s][^:\\n;]*$",
    "decreaseIndentPattern": "^\\S.*$"
  },
  "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\#\\$\\^\\&\\*\\(\\)\\=\\+\\[\\{\\]\\}\\|\\;\\:\\\"\\'\\,\\.\\<\\>\\/\\?\\s]+)"
}
